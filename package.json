{
  "name": "redflare",
  "version": "0.0.0",
  "description": "Experimental JavaScript static analyzer",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-all && npm run lint",
    "test-all": "ava --verbose test/unit/*.js test/inputs.js test/reporter.js",
    "test-coverage": "nyc npm run test",
    "lint": "eslint ."
  },
  "keywords": [
    "static analyzer",
    "static analysis",
    "static",
    "analyzer",
    "analysis"
  ],
  "bin": {
    "redflare": "bin/redflare.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:iefserge/redflare.git"
  },
  "bugs": {
    "url": "https://github.com/iefserge/redflare/issues"
  },
  "homepage": "https://github.com/iefserge/redflare",
  "author": "Sergii Iefremov <shayief@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "acorn": "^2.7.0",
    "chalk": "^1.1.1",
    "immutable": "^3.7.6",
    "pad": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "eslint": "^2.2.0",
    "eslint-config-runtime": "^2.0.0",
    "nyc": "^5.6.0"
  }
}
